generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pedido {
  id                 String    @id @default(uuid())
  nome               String
  razaoSocial        String
  cpfCnpj            String
  pj                 Boolean?
  quantidadeDiarias  Int
  nomeEdificio       String
  moradorDoEdificio  Boolean
  pedidoId           String
  dataPedido         DateTime
  dataPublicacao     DateTime 
  criadoEm           DateTime @default(now())

  @@map("pedidos")
}

